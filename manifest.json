{
	"version"		: "0.0.0.9",
	"manifest_version"	: 3,
	"name"			: "Flickr-BIS",
	"description"		: "Better Image Search for Flickr",
	"action"	: {
		"default_icon"		: "icon/icon.png",
		"default_popup"		: "html/popup.html"
	},
	"host_permissions": [
		"*://*.flickr.com/*"
	],
	"content_security_policy": {
		"script-src": "self",
		"object-src": "self"
	}
}
