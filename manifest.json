{
	"version"		: "0.0.0.6",
	"manifest_version"	: 2,
	"name"			: "Flickr-BIS",
	"description"		: "Better Image Search for Flickr",
	"browser_action"	: {
		"default_icon"		: "icon/icon.png",
		"default_popup"		: "html/popup.html"
	},
	"permissions": [
		"*://*.flickr.com/",
		"tabs"
	],
	"content_security_policy": "script-src 'self' chrome-extension://; object-src 'self' chrome-extension-resource://;"
}