{
	"version"		: "0.0.0.10",
	"manifest_version"	: 3,
	"name"			: "Flickr-BIS",
	"description"		: "Better image search for Flickr",
	"action"	: {
		"default_icon"		: "icon/icon.png",
		"default_popup"		: "html/popup.html"
	},
	"host_permissions": [
		"*://*.flickr.com/*"
	],
	"content_security_policy": {
		"script-src": "self",
		"object-src": "self"
	},
	"icons": {
		"16": "icon/icon16.png",
		"32": "icon/icon32.png",
		"48": "icon/icon48.png",
		"128": "icon/icon128.png"
	}
}
